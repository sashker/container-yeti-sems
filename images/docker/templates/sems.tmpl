node_id = {{env.Getenv "SEMS_NODE_ID"}}

interfaces=intern
sip_ip_intern={{env.Getenv "SEMS_SIP_IP"}}
public_ip_intern={{env.Getenv "SEMS_PUBLIC_IP"}}
sip_port_intern={{env.Getenv "SEMS_SIP_PORT"}}
media_ip_intern={{env.Getenv "SEMS_SIP_IP"}}
rtp_low_port_intern={{env.Getenv "SEMS_RTP_LOW_PORT"}}
rtp_high_port_intern={{env.Getenv "SEMS_RTP_HIGH_PORT"}}
plugin_path=/usr/lib/sems/plug-in/
load_plugins={{env.Getenv "SEMS_PLUGINS"}}
application = yeti
plugin_config_path=/etc/sems/etc/
fork={{env.Getenv "SEMS_FORK"}}
stderr={{env.Getenv "SEMS_STDERR"}}
syslog_loglevel=2
max_shutdown_time = 10

session_processor_threads=20
media_processor_threads=2
session_limit="4000;509;Node overloaded"
shutdown_mode_reply="508 Node in shutdown mode"
options_session_limit="900;503;Warning, server soon overloaded"
# cps_limit="100;503;Server overload"
use_raw_sockets=yes
sip_timer_B = 8000
default_bl_ttl=0
registrations_enabled={{env.Getenv "SEMS_REGISTRATIONS_ENABLED"}}

use_default_signature=no
signature={{env.Getenv "SEMS_SIGNATURE"}}
